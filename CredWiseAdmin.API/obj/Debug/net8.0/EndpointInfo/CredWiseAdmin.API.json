  "components": {
    "schemas": {
      "AdminLoginDto": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "EmiPlanDto": {
        "required": [
          "interestRate",
          "loanId",
          "tenureInMonths"
        ],
        "type": "object",
        "properties": {
          "loanId": {
            "type": "integer",
            "format": "int32"
          },
          "tenureInMonths": {
            "maximum": 24,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "interestRate": {
            "maximum": 100,
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "loanAmount": {
            "type": "number",
            "format": "double"
          },
          "startDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "GoldLoanDetailDto": {
        "required": [
          "goldPurityRequired",
          "repaymentType"
        ],
        "type": "object",
        "properties": {
          "interestRate": {
            "type": "number",
            "format": "double"
          },
          "tenureMonths": {
            "type": "integer",
            "format": "int32"
          },
          "processingFee": {
            "type": "number",
            "format": "double"
          },
          "goldPurityRequired": {
            "type": "string",
            "nullable": true
          },
          "repaymentType": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "HomeLoanDetailDto": {
        "type": "object",
